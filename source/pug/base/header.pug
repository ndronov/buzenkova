mixin header()
    header.header
        a(href='#', data-os='fade-right')
            svg.header__logo(xmlns="http://www.w3.org/2000/svg", xmlns:xlink="http://www.w3.org/1999/xlink", viewBox="0 0 1000 1000")
                use(xlink:href="img/sprite.svg#logo", href="img/sprite.svg#logo")
        .header-mob
            .burger-menu
                nav.nav
                    button.nav__button(id="nav__button")
                        span.toggle-menu(data-aos="fade-left")
                        span.line.line1
                        span.line.line2
                        span.line.line3
                    .nav__overlay
                        .nav__content
                            div
                                ul.nav__list
                                    li.nav__item
                                        a.nav__name(href="#team") О нас
                                    li.nav__item
                                        a.nav__name(href="#portfolio") Портфолио
                                    li.nav__item
                                        a.nav__name(href="#price") Услуги и цены
                                    li.nav__item
                                        a.nav__name(href="#faq") FAQ
                                    li.nav__item
                                        a.nav__name(href="#reviews") Отзывы
                                    li.nav__item
                                        a.nav__name(href="#contacts") Контакты
                                    li.nav__item
                                        a.nav__name(href="#calc") Оставить заявку
        .header-desk
            nav.header__desk__nav
                ul.header__desk__list
                    li.header-desk__item
                        a.header-desk__item(href="#team") О нас
                    li.header-desk__item
                        a.header-desk__item(href="#portfolio") Портфолио
                    li.header-desk__item
                        a.header-desk__item(href="#price") Услуги и цены
                    li.header-desk__item
                        a.header-desk__item(href="#faq") FAQ
                    li.header-desk__item
                        a.header-desk__item(href="#reviews") Отзывы
                    li.header-desk__item
                        a.header-desk__item(href="#contacts") Контакты
                    li.header-desk__item
                        a.header-desk__item(href="#calc") Оставить заявку

extends ../base/layout

block variables
  - var title = "Портфолио"
  - var currentPage = 'portfolio'
  - var isInnerPage = true
  - var albumDate = '11.11.2021'
  - var albumsArray = PARTIES_COVERS
  - var album = albumsArray.find((el) => el.date === albumDate)
  - var albumNumber = album.coverSrc.replace(/\D+/g, '')
  - var albumMod = albumDate.replaceAll('.', '')

block main
    +album(album, albumNumber, albumMod, 44)
